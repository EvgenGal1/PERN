{
  "name": "pern-start_front",
  "version": "1.0.0",
  "description": "PERN Stack Frontend (PostgreSQL, Express, React, NodeJS)",
  "main": "index.tsx",
  "author": "EvGen Gal <ZBst.1@yandex.ru>",
  "license": "ISC",
  "keywords": [
    "PERN",
    "React",
    "TypeScript"
  ],
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start_0": "react-scripts start",
    "build_0": "react-scripts build",
    "test_0": "react-scripts test",
    "start": "craco start",
    "build": "craco build",
    "build_2": "craco build --max-old-space-size=4096",
    "start_2": "craco start -a 0.0.0.0",
    "serve": "serve -s build",
    "prod": "npm run build && npm run serve",
    "test": "craco test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:cl": "eslint . --cache --cache-location .eslintcache",
    "lint:fix": "eslint . --fix",
    "lint-cc": "powershell -Command \"if (Test-Path .eslintcache) { Remove-Item .eslintcache -Force }\"",
    "format": "prettier --write 'src/**/*.{js,js,ts,tsx,css,scss}'",
    "ts-check": "tsc --noEmit",
    "ts-cc": "tsc --build --clean",
    "ci-check": "npm run lint & npm run ts-check & npm test",
    "ci-tsff": "npm run ts-check & npm run lint:fix & npm run format",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit"
    ]
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "jwt-decode": "^4.0.0",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.20",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.15.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react-scripts": "^5.0.1",
    "react-window": "^1.8.11",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "typescript": "^4.9.5",
    "use-debounce": "^10.0.4",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
