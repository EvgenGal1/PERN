{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "includeFiles": ["build/**"],
        "cleanUrls": true,
        "maxOldSpaceSize": 4096
      }
    }
  ],
  "output": "build",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
